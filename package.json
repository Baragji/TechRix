{
  "name": "techflow-nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "dev:fresh": "rm -rf .next && next dev --turbopack --port 3000",
    "dev:no-cache": "NEXT_PRIVATE_DISABLE_CACHE=1 next dev --turbopack --port 3000",
    "dev:network": "next dev --turbopack --hostname 0.0.0.0 --port 3000",
    "dev:ultra-fast": "NEXT_PRIVATE_DISABLE_CACHE=1 NEXT_PRIVATE_SKIP_STATIC_OPTIMIZATION=1 next dev --turbopack --port 3000",
    "dev:clean": "rm -rf .next && rm -rf node_modules/.cache && next dev --turbopack --port 3000",
    "dev:custom": "node scripts/dev-server-config.js",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up --build",
    "dev:tunnel": "npm run dev & npx ngrok http 3000",
    "clear-cache": "node tools/scripts/clear-dev-cache.js",
    "dev:vercel": "vercel dev --listen 3000",
    "dev:vercel-public": "vercel dev --listen 0.0.0.0:3000",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "dev:debug-brk": "NODE_OPTIONS='--inspect-brk' next dev --turbopack",
    "build": "next build",
    "build:debug": "NODE_OPTIONS='--inspect' next build",
    "build:memory-debug": "next build --experimental-debug-memory-usage",
    "start": "next start",
    "start:debug": "NODE_OPTIONS='--inspect' next start",
    "lint": "eslint . --max-warnings=10",
    "lint:report": "eslint . -f html -o hydration-report.html",
    "lint:hydration": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "analyze": "ANALYZE=true npm run build",
    "audit:performance": "node tools/scripts/performance-audit.js",
    "build:production": "NODE_ENV=production npm run build",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html --chrome-flags='--headless'",
    "health-check": "node tools/scripts/health-check.js",
    "optimize": "node tools/scripts/optimize.js",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "build:analyze": "ANALYZE=true npm run build",
    "test:lighthouse": "npm run build && npm start & sleep 5 && npm run lighthouse && kill %1",
    "precommit": "npm run type-check && npm run lint && npm run format:check",
    "fix-runtime": "node tools/scripts/fix-runtime-issues.js",
    "validate-images": "node tools/scripts/validate-images.js",
    "dev:safe": "npm run fix-runtime && npm run dev",
    "build:safe": "npm run fix-runtime && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:staged": "jest --bail --findRelatedTests --passWithNoTests",
    "test:a11y": "jest --testPathPatterns=a11y",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "validate:html": "html-validate 'src/**/*.{tsx,jsx}'",
    "validate:a11y": "pa11y http://localhost:3000",
    "validate:all": "npm run validate:html && npm run test:a11y",
    "debug:comprehensive": "npm run validate:all && npm run test:coverage && npm run lint",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "quality:check": "npm run type-check && npm run lint && npm run test:ci",
    "quality:fix": "npm run lint:fix && npm run format"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.18.1",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "react-intersection-observer": "^9.16.0",
    "swiper": "^11.2.8",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.67"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@axe-core/webdriverjs": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.3.4",
    "@next/eslint-plugin-next": "^15.3.4",
    "@tailwindcss/postcss": "^4.1.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "cypress-audit": "^1.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-ssr-friendly": "^1.3.0",
    "html-validate": "^9.7.0",
    "husky": "^9.0.11",
    "jest": "^30.0.2",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.2",
    "lint-staged": "^15.2.10",
    "pa11y": "^9.0.0",
    "postcss-import": "^16.1.1",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.10",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "jest": "./config/jest/jest.config.mjs"
}
